<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
	minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<columnConstraints>
		<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
			prefWidth="100.0" />
		<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
			prefWidth="100.0" />
	</columnConstraints>
	<rowConstraints>
		<RowConstraints maxHeight="1.7976931348623157E308"
			minHeight="10.0" prefHeight="1000.0" vgrow="SOMETIMES" />
		<RowConstraints maxHeight="1.7976931348623157E308"
			minHeight="-Infinity" vgrow="SOMETIMES" />
	</rowConstraints>
	<children>
		<ListView fx:id="listProjects" prefHeight="1000.0" prefWidth="200.0">
			<GridPane.margin>
				<Insets bottom="20.0" left="40.0" right="40.0" top="40.0" />
			</GridPane.margin>
		</ListView>
		<VBox prefHeight="0.0" prefWidth="100.0" spacing="20.0"
			GridPane.rowIndex="1">
			<children>
				<HBox alignment="CENTER" prefHeight="0.0" prefWidth="200.0"
					spacing="40.0">
					<children>
						<CheckBox fx:id="checkEarly" minHeight="-Infinity"
							mnemonicParsing="false" prefHeight="30.0" selected="true"
							text="Frühe Projekte anzeigen" />
						<CheckBox fx:id="checkLate" minHeight="-Infinity"
							mnemonicParsing="false" prefHeight="30.0" selected="true"
							text="Späte Projekte anzeigen" />
						<CheckBox fx:id="checkFull" minHeight="-Infinity"
							mnemonicParsing="false" prefHeight="30.0" selected="true"
							text="Ganze Projekte anzeigen" />
					</children>
				</HBox>
				<TextField fx:id="textSearch" minHeight="-Infinity"
					prefHeight="30.0" promptText="Suche" />
			</children>
			<GridPane.margin>
				<Insets bottom="40.0" left="40.0" right="40.0" top="20.0" />
			</GridPane.margin>
		</VBox>
		<VBox alignment="CENTER" prefHeight="0.0" prefWidth="100.0"
			spacing="20.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
			<children>
				<Label fx:id="labelState" minHeight="-Infinity" prefHeight="30.0"
					style="-fx-font-size: 15;" text="{STATUS}">
					<VBox.margin>
						<Insets />
					</VBox.margin>
				</Label>
				<Button fx:id="btnDoChoice" minHeight="-Infinity"
					mnemonicParsing="false" prefHeight="30.0" prefWidth="1000.0"
					text="Wahl abschicken" VBox.vgrow="ALWAYS">
					<VBox.margin>
						<Insets />
					</VBox.margin>
				</Button>
			</children>
			<GridPane.margin>
				<Insets bottom="40.0" left="40.0" right="40.0" top="20.0" />
			</GridPane.margin>
		</VBox>
		<VBox alignment="CENTER" prefHeight="200.0" prefWidth="1000.0"
			GridPane.columnIndex="1">
			<children>
				<Label minHeight="-Infinity" prefHeight="30.0" style="-fx-font-size: 15;"
					text="Erst-Wahl">
					<VBox.margin>
						<Insets bottom="20.0" left="40.0" right="40.0" top="40.0" />
					</VBox.margin>
				</Label>
				<Button fx:id="btnSelectFirst" minHeight="-Infinity"
					mnemonicParsing="false" prefHeight="30.0" prefWidth="1000.0"
					text="Ausgewähltes Projekt wählen">
					<VBox.margin>
						<Insets bottom="20.0" left="40.0" right="40.0" />
					</VBox.margin>
				</Button>
				<GridPane gridLinesVisible="true">
					<children>
						<Label fx:id="labelFirstEarly" minHeight="-Infinity"
							prefHeight="30.0" style="-fx-font-size: 15;" text="{VALUE}"
							GridPane.columnIndex="1" />
						<Label fx:id="labelFirstLate" minHeight="-Infinity"
							prefHeight="30.0" style="-fx-font-size: 15;" text="{VALUE}"
							GridPane.columnIndex="1" GridPane.rowIndex="1" />
						<Label minHeight="-Infinity" prefHeight="30.0" style="-fx-font-size: 15;"
							text="Ganze Wahl" GridPane.rowIndex="2" />
						<Label minHeight="-Infinity" prefHeight="30.0" style="-fx-font-size: 15;"
							text="Späte Wahl" GridPane.rowIndex="1" />
						<Label minHeight="-Infinity" prefHeight="30.0" style="-fx-font-size: 15;"
							text="Frühe Wahl" />
						<Label fx:id="labelFirstFull" minHeight="-Infinity"
							prefHeight="30.0" style="-fx-font-size: 15;" text="{VALUE}"
							GridPane.columnIndex="1" GridPane.rowIndex="2" />
					</children>
					<columnConstraints>
						<ColumnConstraints halignment="CENTER" hgrow="SOMETIMES"
							minWidth="-Infinity" prefWidth="110.0" />
						<ColumnConstraints halignment="CENTER" hgrow="SOMETIMES"
							minWidth="10.0" prefWidth="1000.0" />
					</columnConstraints>
					<rowConstraints>
						<RowConstraints minHeight="-Infinity" prefHeight="30.0"
							vgrow="SOMETIMES" />
						<RowConstraints minHeight="-Infinity" prefHeight="30.0"
							vgrow="SOMETIMES" />
						<RowConstraints minHeight="-Infinity" prefHeight="30.0"
							vgrow="SOMETIMES" />
					</rowConstraints>
					<VBox.margin>
						<Insets bottom="40.0" left="40.0" right="40.0" />
					</VBox.margin>
				</GridPane>
				<Separator prefWidth="200.0">
					<VBox.margin>
						<Insets left="20.0" right="20.0" />
					</VBox.margin>
				</Separator>
				<Label minHeight="-Infinity" prefHeight="30.0" style="-fx-font-size: 15;"
					text="Zweit-Wahl">
					<VBox.margin>
						<Insets bottom="20.0" left="40.0" right="40.0" top="40.0" />
					</VBox.margin>
				</Label>
				<Button fx:id="btnSelectSecond" minHeight="-Infinity"
					mnemonicParsing="false" prefHeight="30.0" prefWidth="1000.0"
					text="Ausgewähltes Projekt wählen">
					<VBox.margin>
						<Insets bottom="20.0" left="40.0" right="40.0" />
					</VBox.margin>
				</Button>
				<GridPane gridLinesVisible="true">
					<children>
						<Label fx:id="labelSecondEarly" minHeight="-Infinity"
							prefHeight="30.0" style="-fx-font-size: 15;" text="{VALUE}"
							GridPane.columnIndex="1" />
						<Label fx:id="labelSecondLate" minHeight="-Infinity"
							prefHeight="30.0" style="-fx-font-size: 15;" text="{VALUE}"
							GridPane.columnIndex="1" GridPane.rowIndex="1" />
						<Label minHeight="-Infinity" prefHeight="30.0" style="-fx-font-size: 15;"
							text="Ganze Wahl" GridPane.rowIndex="2" />
						<Label minHeight="-Infinity" prefHeight="30.0" style="-fx-font-size: 15;"
							text="Späte Wahl" GridPane.rowIndex="1" />
						<Label minHeight="-Infinity" prefHeight="30.0" style="-fx-font-size: 15;"
							text="Frühe Wahl" />
						<Label fx:id="labelSecondFull" minHeight="-Infinity"
							prefHeight="30.0" style="-fx-font-size: 15;" text="{VALUE}"
							GridPane.columnIndex="1" GridPane.rowIndex="2" />
					</children>
					<columnConstraints>
						<ColumnConstraints halignment="CENTER" hgrow="SOMETIMES"
							minWidth="-Infinity" prefWidth="110.0" />
						<ColumnConstraints halignment="CENTER" hgrow="SOMETIMES"
							minWidth="10.0" prefWidth="1000.0" />
					</columnConstraints>
					<rowConstraints>
						<RowConstraints minHeight="-Infinity" prefHeight="30.0"
							vgrow="SOMETIMES" />
						<RowConstraints minHeight="-Infinity" prefHeight="30.0"
							vgrow="SOMETIMES" />
						<RowConstraints minHeight="-Infinity" prefHeight="30.0"
							vgrow="SOMETIMES" />
					</rowConstraints>
					<VBox.margin>
						<Insets bottom="40.0" left="40.0" right="40.0" />
					</VBox.margin>
				</GridPane>
				<Separator prefWidth="200.0">
					<VBox.margin>
						<Insets left="20.0" right="20.0" />
					</VBox.margin>
				</Separator>
				<Label minHeight="-Infinity" prefHeight="30.0" style="-fx-font-size: 15;"
					text="Dritt-Wahl">
					<VBox.margin>
						<Insets bottom="20.0" left="40.0" right="40.0" top="40.0" />
					</VBox.margin>
				</Label>
				<Button fx:id="btnSelectThird" minHeight="-Infinity"
					mnemonicParsing="false" prefHeight="30.0" prefWidth="1000.0"
					text="Ausgewähltes Projekt wählen">
					<VBox.margin>
						<Insets bottom="20.0" left="40.0" right="40.0" />
					</VBox.margin>
				</Button>
				<GridPane gridLinesVisible="true">
					<children>
						<Label fx:id="labelThirdEarly" minHeight="-Infinity"
							prefHeight="30.0" style="-fx-font-size: 15;" text="{VALUE}"
							GridPane.columnIndex="1" />
						<Label fx:id="labelThirdLate" minHeight="-Infinity"
							prefHeight="30.0" style="-fx-font-size: 15;" text="{VALUE}"
							GridPane.columnIndex="1" GridPane.rowIndex="1" />
						<Label minHeight="-Infinity" prefHeight="30.0" style="-fx-font-size: 15;"
							text="Ganze Wahl" GridPane.rowIndex="2" />
						<Label minHeight="-Infinity" prefHeight="30.0" style="-fx-font-size: 15;"
							text="Späte Wahl" GridPane.rowIndex="1" />
						<Label minHeight="-Infinity" prefHeight="30.0" style="-fx-font-size: 15;"
							text="Frühe Wahl" />
						<Label fx:id="labelThirdFull" minHeight="-Infinity"
							prefHeight="30.0" style="-fx-font-size: 15;" text="{VALUE}"
							GridPane.columnIndex="1" GridPane.rowIndex="2" />
					</children>
					<columnConstraints>
						<ColumnConstraints halignment="CENTER" hgrow="SOMETIMES"
							minWidth="-Infinity" prefWidth="110.0" />
						<ColumnConstraints halignment="CENTER" hgrow="SOMETIMES"
							minWidth="10.0" prefWidth="1000.0" />
					</columnConstraints>
					<rowConstraints>
						<RowConstraints minHeight="-Infinity" prefHeight="30.0"
							vgrow="SOMETIMES" />
						<RowConstraints minHeight="-Infinity" prefHeight="30.0"
							vgrow="SOMETIMES" />
						<RowConstraints minHeight="-Infinity" prefHeight="30.0"
							vgrow="SOMETIMES" />
					</rowConstraints>
					<VBox.margin>
						<Insets bottom="20.0" left="40.0" right="40.0" />
					</VBox.margin>
				</GridPane>
			</children>
		</VBox>
	</children>
</GridPane>
