<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<ListView fx:id="listStudents" prefHeight="1000.0" prefWidth="1000.0">
			<VBox.margin>
				<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
			</VBox.margin>
		</ListView>
		<HBox prefHeight="100.0" prefWidth="200.0" spacing="20.0">
			<children>
				<ComboBox fx:id="boxClasses" minHeight="-Infinity" prefHeight="30.0" prefWidth="1000.0" promptText="Klasse">
					<HBox.margin>
						<Insets />
					</HBox.margin>
				</ComboBox>
				<TextField fx:id="textSearch" minHeight="-Infinity" prefHeight="30.0" prefWidth="1000.0" promptText="Suche">
					<HBox.margin>
						<Insets />
					</HBox.margin>
				</TextField>
			</children>
			<VBox.margin>
				<Insets bottom="10.0" left="10.0" right="10.0" />
			</VBox.margin>
		</HBox>
	</children>
</VBox>
