<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="0.0" prefWidth="0.0" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<ListView fx:id="listTeachers" maxHeight="-Infinity"
			maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
			prefHeight="200.0" prefWidth="300.0">
			<VBox.margin>
				<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
			</VBox.margin>
		</ListView>
		<TextField fx:id="textSearch" maxHeight="-Infinity"
			maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
			prefHeight="30.0" prefWidth="300.0" promptText="Name des Lehrers">
			<VBox.margin>
				<Insets bottom="10.0" left="10.0" right="10.0" />
			</VBox.margin>
		</TextField>
	</children>
</VBox>
